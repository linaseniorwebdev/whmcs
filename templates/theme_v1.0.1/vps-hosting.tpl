<script type="text/javascript"> 
 {foreach from=$slider_values key=name  item=val}
  	{$val}
  {/foreach}
  // This is what you want the default position to be
var def_pos = 3;
$(document).ready(function(){
	var updateServerstake= function(val){
		var stack = $("div#vps_server");
			stack.empty();
			var offset;
			var offsetleft;
			for (i = 0; i <val; i++) {
				offset = 31 * i;
				offsetleft = 0 * i;
				stack.append($('<div>')
					.addClass('server')
					.css('bottom', offset).css('left', offsetleft));
			}
}	
	
	var updatevalues= function( val){
		$('.slider-container #CPU').html(CPU[val-1]);
		$('.slider-container #RAM').html(RAM[val-1]);
		$('.slider-container #STORAGE').html(STORAGE[val-1]);
		$('.slider-container #Bandwidth').html(Bandwidth[val-1]);
		$('#price_val').html(Price[val-1]);
		$(' a.buynow-button').attr('href', 'cart.php?a=add&pid='+id[val-1]);
		
		$("div.price_rangetxt div").removeClass("current");
		for(var i=0;i<val;i++)
			$(".price_rangetxt div#icon-"+i).addClass("current");
		updateServerstake(val);
	}
	
	$( "#slider" ).slider({
		range: 'min',
		animate: true,
		min: 1,
		max: 4,
		paddingMin: 135,
		paddingMax: 115,
		//step: 0.1,
		slide: function( event, ui ) {
			updatevalues(ui.value);
			
		},
		change: function( event, ui ) {
			updatevalues(ui.value);
		}
	  });
	$('#slider').slider('value', def_pos);
	$('.icon').click(function() {
	ch_value= parseInt(this.id.slice(5)) + 1;
	$(".slider-container div.price_rangetxt div").removeClass("current");
	$('#slider').slider('value', ch_value);
});
});
</script> 
